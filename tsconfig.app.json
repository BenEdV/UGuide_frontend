{
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "@shared/*": [
        "src/app/shared/*"
      ]
    },
    "types": []
  },
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ],
  "extends": "./tsconfig.base.json",
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
